<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alessandro Cagliostro Goncalves Neves</Authors>
    <Description>CryptHash.Net is a .NET Standard Class Library for text and files symmetric (AES 256 CBC / AES 256 GCM) encryption/decryption with salt and authentication support, and text/files hashing (MD5, SHA1, SHA256, SHA384, SHA512, BCrypt and PBKDF2).</Description>
    <Copyright>Alessandro Cagliostro Goncalves Neves, 2019</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIconUrl>https://raw.githubusercontent.com/alecgn/crypthash-net/master/src/CryptHash.Net/lib/CryptHash.Net/Icon/crypthash-icon.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/alecgn/crypthash-net</RepositoryUrl>
    <PackageTags>encryption decryption cryptography aes aes256 aes256cbc aescbc aescbc256 cbc aes256gcm aesgcm aesgcm256 gcm hash md5 sha1 sha256 sha384 sha512 bcrypt pbkdf2 c# c-sharp security password authentication authenticated</PackageTags>
    <AssemblyVersion>1.0.2</AssemblyVersion>
    <FileVersion>1.0.2</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/alecgn/crypthash-net</PackageProjectUrl>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Next" Version="3.1.3" />
  </ItemGroup>

</Project>
